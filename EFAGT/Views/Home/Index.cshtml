
@model EFAGT.Models.ViewModele
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<asp:Content id="Content1" ContentPlaceHolderID="MainContent" runat="server">
    <div class="row">
        <table>
            <tr>

                <th>
                    Numero de ticket
                </th>

            </tr>

            @foreach (var item in Model.visualisationTickets) {//boucle permettant d'afficher le contenu de la table
            <tr>
                <td>
                    @Html.EditorFor(modele => modele.visualisationTickets.FirstOrDefault().idTicket)
                </td>
            </tr>
            }
        </table>
    </div>

</asp:Content>